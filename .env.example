# Victory36 Environment Configuration Template
# Classification: Diamond SAO Only
# Copy this file to .env and update with actual values

# ==============================================
# SECURITY CONFIGURATION (DIAMOND SAO)
# ==============================================

# Diamond SAO Authentication Token (REQUIRED)
DIAMOND_SAO_TOKEN=your-diamond-sao-token-here

# Diamond SAO Key ID for encryption
DIAMOND_SAO_KEY_ID=victory36-production-key

# Biometric validation endpoint
BIOMETRIC_VALIDATION_ENDPOINT=https://auth.asoos.cloud/biometric

# Hardware Security Module configuration
HSM_ENDPOINT=https://hsm.asoos.cloud
HSM_KEY_ID=victory36-hsm-key

# ==============================================
# SYSTEM CONFIGURATION
# ==============================================

# Environment (development, staging, production)
NODE_ENV=production

# Maximum number of concurrent AI agents
MAX_AGENTS=20000000

# Supported regions (comma-separated)
REGIONS=MOCOA,MOCORIX,MOCORIX2

# Primary region for operations
PRIMARY_REGION=MOCOA

# ==============================================
# CONNECTION POOL CONFIGURATION
# ==============================================

# Connection pool strategy (round-robin, least-connections, regional-affinity)
LOAD_BALANCING_STRATEGY=least-connections

# Minimum connections per region
MIN_CONNECTIONS_PER_REGION=10000

# Connection timeout in milliseconds
CONNECTION_TIMEOUT=5000

# Idle timeout in milliseconds
IDLE_TIMEOUT=300000

# Maximum retry attempts
MAX_RETRY_ATTEMPTS=3

# Health check interval in milliseconds
HEALTH_CHECK_INTERVAL=30000

# ==============================================
# PERFORMANCE CONFIGURATION
# ==============================================

# CPU allocation per region (in millicores)
CPU_ALLOCATION=4000

# Memory allocation per region (in MB)
MEMORY_ALLOCATION=8192

# Network buffer size
NETWORK_BUFFER_SIZE=65536

# Maximum concurrent requests per second
MAX_RPS=150000

# ==============================================
# MONITORING AND LOGGING
# ==============================================

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Log retention period in days
LOG_RETENTION_DAYS=30

# Metrics collection interval in seconds
METRICS_INTERVAL=10

# Dashboard refresh interval in milliseconds
DASHBOARD_REFRESH_INTERVAL=5000

# ==============================================
# CLOUD INFRASTRUCTURE
# ==============================================

# Google Cloud Project ID
GCP_PROJECT_ID=asoos-victory36

# Google Cloud Region
GCP_REGION=us-west1

# Kubernetes cluster name
K8S_CLUSTER_NAME=victory36-cluster

# Namespace for canary deployments
CANARY_NAMESPACE=v36-canary

# Namespace for production deployments
PRODUCTION_NAMESPACE=v36-prod

# ==============================================
# DATABASE CONFIGURATION
# ==============================================

# Firestore database ID
FIRESTORE_DATABASE_ID=victory36-db

# Redis connection URL for caching
REDIS_URL=redis://redis.mocoa.asoos.cloud:6379

# Connection pool for database
DB_POOL_SIZE=100

# ==============================================
# API CONFIGURATION
# ==============================================

# API server port
PORT=8080

# API rate limiting (requests per minute)
RATE_LIMIT=1000

# CORS allowed origins
CORS_ORIGINS=https://v36-dashboard.mocoa.asoos.cloud,https://admin.asoos.cloud

# JWT secret for API authentication
JWT_SECRET=your-jwt-secret-here

# API version
API_VERSION=v1

# ==============================================
# SECURITY THRESHOLDS
# ==============================================

# Minimum success rate threshold (%)
MIN_SUCCESS_RATE=99.5

# Maximum error rate threshold (%)
MAX_ERROR_RATE=0.5

# Maximum response time threshold (ms)
MAX_RESPONSE_TIME=100

# Memory usage alert threshold (%)
MEMORY_ALERT_THRESHOLD=85

# CPU usage alert threshold (%)
CPU_ALERT_THRESHOLD=80

# ==============================================
# AGENT TIER CONFIGURATION
# ==============================================

# Elite 11 agent priority weight
ELITE_11_PRIORITY=10

# Mastery 33 agent priority weight
MASTERY_33_PRIORITY=5

# Standard agent priority weight
STANDARD_PRIORITY=1

# Elite 11 dedicated resources (%)
ELITE_11_RESOURCE_ALLOCATION=20

# Mastery 33 dedicated resources (%)
MASTERY_33_RESOURCE_ALLOCATION=15

# ==============================================
# DEPLOYMENT CONFIGURATION
# ==============================================

# Canary traffic percentage
CANARY_TRAFFIC_PERCENTAGE=5

# Canary validation period (hours)
CANARY_VALIDATION_HOURS=24

# Blue-green deployment enabled
BLUE_GREEN_ENABLED=true

# Rollback timeout (seconds)
ROLLBACK_TIMEOUT=300

# ==============================================
# ALERTING CONFIGURATION
# ==============================================

# Slack webhook URL for alerts
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-webhook-url

# PagerDuty integration key
PAGERDUTY_INTEGRATION_KEY=your-pagerduty-key

# Email alerts recipient
ALERT_EMAIL=ops@asoos.cloud

# SMS alerts phone number
ALERT_PHONE=+1-555-DIAMOND

# ==============================================
# BACKUP AND RECOVERY
# ==============================================

# Backup interval (hours)
BACKUP_INTERVAL=6

# Backup retention period (days)
BACKUP_RETENTION_DAYS=90

# Disaster recovery region
DR_REGION=MOCORIX2

# Recovery time objective (minutes)
RTO=30

# Recovery point objective (minutes)
RPO=15

# ==============================================
# COMPLIANCE AND AUDIT
# ==============================================

# Audit logging enabled
AUDIT_LOGGING_ENABLED=true

# Compliance framework (SOC2, ISO27001, NIST)
COMPLIANCE_FRAMEWORK=SOC2

# Data encryption at rest enabled
ENCRYPTION_AT_REST=true

# Data encryption in transit enabled
ENCRYPTION_IN_TRANSIT=true

# ==============================================
# FEATURE FLAGS
# ==============================================

# Quantum-ready cryptography enabled
QUANTUM_CRYPTO_ENABLED=false

# AI-native operations enabled
AI_NATIVE_OPS_ENABLED=true

# Predictive scaling enabled
PREDICTIVE_SCALING_ENABLED=true

# Auto-healing enabled
AUTO_HEALING_ENABLED=true

# ==============================================
# DEVELOPMENT AND TESTING
# ==============================================

# Enable debug mode
DEBUG=false

# Mock external services for testing
MOCK_EXTERNAL_SERVICES=false

# Test data generation enabled
TEST_DATA_ENABLED=false

# Load testing endpoints enabled
LOAD_TEST_ENDPOINTS=false

# Performance profiling enabled
PROFILING_ENABLED=false
